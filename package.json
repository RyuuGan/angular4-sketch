{
  "name": "YOURNAMEHERE",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "cd server && node run.js",
    "eslint": "eslint server",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean-install && npm start",
    "watch": "webpack --watch --progress --profile",
    "build": "rimraf client/dist && webpack --progress --profile --bail",
    "serverClient": "webpack-dev-server --inline --progress --port 3000",
    "lint": "tslint --force \"src/**/*.ts\"",
    "pretest": "npm run lint",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "docs": "typedoc --options typedoc.json src/app/app.component.ts",
    "startClient": "npm run serverClient",
    "start:hmr": "npm run server -- --hot",
    "build:tsc": "tsc -p .",
    "build:tsc:watch": "tsc -w -p . ",
    "extractLocale": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/*.json --sort --format json",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "8.x.x"
  },
